{
  "cli": {
    "version": ">= 5.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "node": "20.19.4",
      "env": {
        "EXPO_USE_FAST_RESOLVER": "1"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "distribution": "internal",
      "node": "20.19.4",
      "env": {
        "EXPO_USE_FAST_RESOLVER": "1"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "node": "20.19.4",
      "cache": {
        "disabled": false,
        "clear": true,
        "paths": []
      },
      "env": {
        "EXPO_USE_FAST_RESOLVER": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
